{
  "name": "less-to-tailwind-parser",
  "version": "1.0.0",
  "description": "Parse LESS CSS files from multiple paths, store hierarchical imports in PostgreSQL, and export as Tailwind-compatible CSS",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "db:migrate": "node dist/database/migrations.js",
    "db:reset": "node dist/database/reset.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "keywords": [
    "less",
    "css",
    "tailwind",
    "parser",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "less": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
}
